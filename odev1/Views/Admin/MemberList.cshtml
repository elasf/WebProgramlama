@model List<odev1.Models.Member>

<table class="table">
    <thead>
        <tr>
            <th>Ad Soyad</th>
            <th>Email</th>
            <th>Kayıt Tarihi</th>
            <th>İşlemler</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var member in Model)
        {
            <tr>
                <td>@member.FullName</td>
                <td>@member.User.Email</td>
                <td>@member.KayitTarihi.ToShortDateString()</td>
                <td>
                    <a href="#" class="btn btn-sm btn-info">Detay</a>
                </td>
            </tr>
        }
    </tbody>
</table>